FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00009357;

boundaryField
{
    //- Set patchGroups for constraint patches

    //- Define inlet conditions
    
    inlet
    {
    	type            freestream;
        freestreamValue uniform 0.00009357;
    }

    outlet
    {
        type            freestream;
        freestreamValue uniform 0.00009357;
    }

    topandbottom
    {
        type            freestream;
        freestreamValue uniform 0.00009357;
    }

    sphere
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    frontandback
    {
    	typpe 			empty;
    }

   
}


